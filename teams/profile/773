    <script type="text/javascript">
        $(document).ready(function () {
            $("[data-toggle=tooltip]").tooltip();
            $("ul[role=tablist]").tab();
            $("a[role=tab]").click(function (e) {
                e.preventDefault();
            });
            


            google.charts.load("current", {"packages": ["corechart"]});
            google.charts.setOnLoadCallback(drawChart);

            function drawChart() {
                var data = google.visualization.arrayToDataTable([["Time", "Score"], ["0:00:00", 0], ["2:25:41", 5], ["2:27:41", 15], ["3:08:14", 65], ["3:14:37", 85], ["3:19:39", 110], ["3:25:54", 130], ["3:35:23", 160], ["3:37:58", 190], ["3:40:21", 240], ["3:57:15", 255], ["4:52:15", 330], ["4:56:56", 405], ["8:10:27", 455], ["8:11:34", 535], ["10:45:17", 635], ["11:04:06", 735], ["37:39:00", 805], ["53:13:49", 855], ["53:30:38", 940], ["54:18:45", 1020], ["57:15:31", 1120], ["58:30:09", 1220], ["62:19:00", 1330], ["63:50:47", 1445], ["64:19:32", 1565], ["64:29:27", 1700], ["97:03:19", 1800], ["97:40:56", 1900], ["150:46:51", 2050], ["163:00:06", 2250], ["215:29:40", 2250]]);

                var options = {
                    title: "Score Progression",
                    curveType: "line",
                    legend: {position: "right"},
                    vAxes: { 0: {title: "Score"} },
                    hAxes: { 0: {title: "Time"} },
                };

                var chart = new google.visualization.LineChart(document.getElementById("progression_chart"));

                chart.draw(data, options);
            }
        });

    </script>

