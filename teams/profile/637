                e.preventDefault();
            });
            


            google.charts.load("current", {"packages": ["corechart"]});
            google.charts.setOnLoadCallback(drawChart);

            function drawChart() {
                var data = google.visualization.arrayToDataTable([["Time", "Score"], ["0:00:00", 0], ["0:01:37", 10], ["0:15:31", 110], ["0:15:53", 135], ["0:19:07", 155], ["0:23:53", 205], ["0:25:51", 235], ["0:48:21", 240], ["0:49:37", 315], ["0:55:35", 335], ["1:27:31", 365], ["2:06:34", 500], ["2:07:20", 615], ["3:26:10", 875], ["11:57:19", 925], ["12:52:57", 1005], ["13:47:31", 1105], ["14:32:39", 1155], ["14:32:39", 1230], ["19:46:35", 1330], ["19:47:50", 1400], ["20:05:35", 1430], ["24:19:45", 1730], ["39:59:51", 1930], ["41:59:09", 1945], ["47:00:13", 2120], ["50:43:10", 2170], ["67:58:32", 2370], ["69:53:29", 2455], ["73:39:43", 2555], ["73:49:50", 2655], ["85:54:28", 2775], ["114:20:34", 2975], ["116:37:32", 3075], ["136:08:47", 3225], ["140:03:18", 3525], ["141:34:06", 3675], ["156:34:53", 3775], ["157:39:43", 4005], ["215:29:20", 4005]]);

                var options = {
                    title: "Score Progression",
                    curveType: "line",
                    legend: {position: "right"},
                    vAxes: { 0: {title: "Score"} },
                    hAxes: { 0: {title: "Time"} },
                };

                var chart = new google.visualization.LineChart(document.getElementById("progression_chart"));

                chart.draw(data, options);
            }
        });

    </script>

</div>
