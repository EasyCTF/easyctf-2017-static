        $(document).ready(function () {
            $("[data-toggle=tooltip]").tooltip();
            $("ul[role=tablist]").tab();
            $("a[role=tab]").click(function (e) {
                e.preventDefault();
            });
            


            google.charts.load("current", {"packages": ["corechart"]});
            google.charts.setOnLoadCallback(drawChart);

            function drawChart() {
                var data = google.visualization.arrayToDataTable([["Time", "Score"], ["0:00:00", 0], ["10:32:06", 10], ["10:43:36", 30], ["10:45:50", 50], ["10:47:38", 75], ["12:29:15", 105], ["12:31:45", 155], ["12:37:54", 225], ["13:05:50", 300], ["21:17:46", 330], ["22:40:07", 430], ["26:08:25", 435], ["27:31:45", 535], ["39:53:14", 635], ["42:02:11", 735], ["42:15:36", 750], ["42:27:03", 800], ["42:44:05", 885], ["47:29:36", 995], ["49:47:36", 1045], ["50:18:38", 1195], ["51:10:15", 1315], ["71:01:43", 1415], ["73:54:34", 1465], ["74:11:58", 1545], ["74:49:50", 1575], ["83:16:10", 1675], ["85:20:16", 1875], ["94:27:53", 1975], ["94:43:44", 2050], ["107:32:48", 2470], ["108:40:36", 2570], ["160:26:43", 2705], ["161:00:12", 2965], ["215:29:29", 2965]]);

                var options = {
                    title: "Score Progression",
                    curveType: "line",
                    legend: {position: "right"},
                    vAxes: { 0: {title: "Score"} },
                    hAxes: { 0: {title: "Time"} },
                };

                var chart = new google.visualization.LineChart(document.getElementById("progression_chart"));

                chart.draw(data, options);
            }
        });

